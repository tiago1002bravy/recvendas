{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webhook/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/webhook",
      "dest": "/api/index.ts"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}

